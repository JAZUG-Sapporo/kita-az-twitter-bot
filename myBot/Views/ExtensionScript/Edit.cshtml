@model myBot.Models.ExtensionScript
@{
    ViewBag.Title = _Localize.Edit;
    ViewBag.GoBackURL = Url.Action("Details", "Bot", new { id = Model.BotID });
    ViewBag.GoBackText = _Localize.Back;
}

@Html.Partial("_ExtensionScriptForm", Model)

@section Scripts {
    @Scripts.Render("~/Scripts/jqueryval")
    <script>
        $(document).ready(function () {
            $('select').material_select();
        });
    </script>
    <script src="~/Views/ExtensionScript/extensionscript-form.js"></script>
}
